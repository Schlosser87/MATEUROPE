#103=[#101*100]-100 
IF[#[15001+#103]LT-9900]GOTO210 
IF[#[15001+#103]GE#110]THEN#102=#102+1
IF[#[15001+#103]LE#111]THEN#102=#102+1
()
N210IF[#[15005+#103]LT-9900]GOTO220 
IF[#[15005+#103]GE#110]THEN#102=#102+1
IF[#[15005+#103]LE#111]THEN#102=#102+1
N220#101=#101+1 
END1
()
(HOME POSITION X/POSITION FOR SAFETY TEST)
IF[#17001GE#110]THEN#102=#102+1 
IF[#17001LE#111]THEN#102=#102+1 
()
(X POSITION MAINT. AREA / POSITION AT M01)
IF[#17002GE#110]THEN#102=#102+1 
IF[#17002LE#111]THEN#102=#102+1 
()
(X-POSITIONS FOR EXTERNAL AUTOMATION) 
#104=1
#105=0
#106=0
WHILE[#104LE#581]DO1
IF[#[520+#104]EQ9]THEN#105=#105+1 
IF[#[520+#104]EQ10]THEN#106=#106+1
#104=#104+1 
END1
()
IF[#105LE0]GOTO240
IF[#830GE#110]THEN#102=#102+1 
IF[#830LE#111]THEN#102=#102+1 
N240IF[#106LE0]GOTO250
IF[#831GE#110]THEN#102=#102+1 
IF[#831LE#111]THEN#102=#102+1 
N250
